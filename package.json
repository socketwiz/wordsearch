{
  "name": "word-search",
  "version": "0.2.2",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "cd frontend && npm run build",
    "lint": "npm run lint:frontend && npm run lint:server",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:server": "cd server && npm run lint",
    "install": "(cd frontend && npm install) && (cd server && npm install)",
    "pretest": "npm run lint",
    "debug:start": "inspect .",
    "start": "cd frontend && npm start",
    "server:start": "cd server && npm start",
    "server:stop": "cd server && npm stop",
    "test": "npm run test:server && npm run test:frontend",
    "test:coverage": "npm run test:server:coverage && npm run test:frontend:coverage",
    "test:frontend": "cd frontend && npm run test:no-watch",
    "test:frontend:coverage": "cd frontend && npm run test:coverage",
    "test:frontend:debug": "cd frontend && npm run test:debug",
    "test:frontend:watch": "cd frontend && npm run test:watch",
    "test:server": "cd server && npm test",
    "test:server:watch": "cd server && npm run test:watch",
    "test:server:coverage": "cd server && npm run test:coverage",
    "test:server:debug": "cd server && npm run debug",
    "updates": "(cd frontend && npm run update && npm install) && (cd server && npm run update && npm install)"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "Web Starter Kit"
}
