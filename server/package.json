{
  "name": "web-starter-kit-server",
  "version": "0.0.1",
  "description": "Word Search Server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "pm2 start index.js --watch",
    "stop": "pm2 stop index.js --watch 0 && pm2 delete 0",
    "prepublishOnly": "npm run test",
    "update": "ncu -u"
  },
  "repository": {
    "type": "git"
  },
  "author": "Ricky Nelson <rickyn@socketwiz.com>",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.1",
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/openapi-v3": "^3.1.0",
    "@loopback/repository": "^2.0.1",
    "@loopback/rest": "^3.0.1",
    "@loopback/rest-explorer": "^2.0.1",
    "@loopback/service-proxy": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.1",
    "@loopback/testlab": "^2.0.1",
    "@types/node": "^13.9.2",
    "eslint": "^6.8.0",
    "npm-check-updates": "4.0.4",
    "pm2": "^4.2.3"
  }
}
